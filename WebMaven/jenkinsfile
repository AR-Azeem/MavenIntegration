pipeline{
    agent any
    stages{
        stage("checkout"){
            steps{
                step
                dir('test123'){
                    git url:"https://github.com/AR-Azeem/test.git" , branch:'master'
                    bat 'pwd'
                }
            }           
        }
        stage("git checkout"){
            steps{
                bat 'cd WebMaven'
                bat 'pwd'
                
            }
        }
    }
}